{
  "dependencies": {
    "prompt": "^0.2.14",
    "string": "^3.3.0"
  },
  "name": "boilerplatify",
  "version": "0.0.48",
  "description": "A boilerplate for my projects",
  "main": "boilerplatify.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/omphalos/boilerplatify"
  },
  "author": "omphalos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/omphalos/boilerplatify/issues"
  },
  "homepage": "https://github.com/omphalos/boilerplatify",
  "scripts": {
    "lint": "./node_modules/.bin/minilint",
    "test": "./node_modules/.bin/istanbul cover ./tests.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "watch": "./node_modules/.bin/nodemon ./tests.js",
    "release": "npm run test && ./node_modules/.bin/bumpt",
    "tape": "./node_modules/.bin/tape ./tests.js"
  },
  "keywords": [
    "boilerplate",
    "utility"
  ],
  "bin": "boilerplatify.js",
  "devDependencies": {
    "bumpt": "0.0.4",
    "coveralls": "^2.11.4",
    "istanbul": "^0.3.20",
    "nodemon": "^1.6.1",
    "tape": "^4.2.0"
  }
}